{
  "name": "shopify-shipping",
  "version": "1.0.0",
  "description": "Shopify shipping management application",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "server:dev": "tsx src/server/app.ts",
    "server:start": "node dist/server/app.js",
    "review:setup": "npm ci --prefix .windsurf/review",
    "review:porcelain": "npm run --prefix .windsurf/review review:porcelain",
    "review:repo": "npm run --prefix .windsurf/review review:repo",
    "review:debug": "npm run --prefix .windsurf/review review:debug"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "axios": "^1.11.0",
    "dotenv": "^16.6.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "next": "^15.4.2",
    "p-retry": "^7.0.0",
    "pg": "^8.12.0",
    "pino": "^9.9.0",
    "playwright": "^1.55.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.0",
    "@types/pg": "^8.11.8",
    "@types/react": "19.1.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.4.2",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.3.3",
    "prisma": "^6.14.0",
    "tsx": "^4.20.5",
    "typescript": "^5.5.4"
  }
}
